<dependencies>
	
	<!-- My Common Module -->
	<dependency>
		<groupId>org.nir.myebuy</groupId>
		<artifactId>MyEbuyCommon</artifactId>
		<version>0.0.1-SNAPSHOT</version>
	</dependency>
	
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-data-rest</artifactId>
